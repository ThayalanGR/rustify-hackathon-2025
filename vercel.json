{
  "version": 2,
  "name": "rust-wasm-react-hackathon",
  "builds": [
    {
      "src": "client/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "client/dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ],
  "installCommand": "npm run build",
  "buildCommand": "cd client && npm run build",
  "outputDirectory": "client/dist"
}