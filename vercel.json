{
  "version": 2,
  "name": "rust-wasm-react-hackathon",
  "buildCommand": "chmod +x ./build.sh && ./build.sh && cd client && npm run build",
  "outputDirectory": "client/dist",
  "installCommand": "cd client && npm install",
  "framework": null,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}