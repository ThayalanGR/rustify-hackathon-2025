{
  "name": "rustify-hackathon-2025",
  "scripts": {
    "build": "./build.sh",
    "dev": "cd client && npm run dev",
    "build:wasm": "cd rust-core && ./build.sh",
    "build:client": "cd client && npm run build",
    "preview": "cd client && npm run preview",
    "lint": "cd client && npm run lint",
    "clean": "rm -rf client/dist client/node_modules rust-core/target client/src/wasm/*.js client/src/wasm/*.wasm client/src/wasm/*.d.ts client/src/wasm/package.json .vercel",
    "clean:wasm": "rm -rf client/src/wasm/*.js client/src/wasm/*.wasm client/src/wasm/*.d.ts client/src/wasm/package.json",
    "install": "cd client && npm install"
  },
  "devDependencies": {
    "@types/node": "^20.0.0"
  }
}